<main>

  <section class="py-5 text-center container">
    <div class="row py-lg-5">
      <div class="col-lg-6 col-md-8 mx-auto">
        <h1 class="fw-light">Registro para nuestro culto del día <%= dias_semana[mi_dia] %> <%= dia_culto %></h1>
        <!--<p class="lead text-muted">Por favor ingrese su número de cédula.</p>-->
        <!--<p>
          <a href="#" class="btn btn-primary my-2">Main call to action</a>
          <a href="#" class="btn btn-secondary my-2">Secondary action</a>
        </p>-->
      </div>
    </div>
  </section>

  <div class="album py-5 bg-light">
    <div class="container">
        <div class="form-row">
        <% if(!mostrarform) { %>    
        <form name="form_registroweb" id="form_registroweb" action="/searchcedula" method="post">
            <div class="form-group col-md-6 mb-2">
                <label for="exampleInputPassword1">Cédula</label>
                <input type="text" name="cedula" class="form-control" id="exampleInputPassword1" placeholder="Cédula">
              </div>
              
              <button type="submit" class="btn btn-primary">Buscar</button>
          </form>
          </div>
        <% } %>

          <% if(mostrarform) { %>
            <form name="form_registroweb" id="form_registroweb" action="/confirmarregistroweb" method="post">
                <div class="form-group col-md-12">
                    <label for="inputEmail4">Cédula</label>
                    <input type="text" readonly class="form-control" id="inputEmail4" placeholder="Cédula" value="<%= results[0].miembro_cedula %>">
                </div>  
                  <div class="form-group col-md-12">
                    <label for="inputEmail4">Nombre</label>
                    <input type="text" readonly class="form-control" id="inputEmail4" placeholder="Nombre" value="<%= results[0].miembro_nombres %>">
                  </div>
                  <div class="form-group col-md-12 mb-2">
                    <label for="inputPassword4">Apellidos</label>
                    <input type="text" readonly class="form-control" id="inputPassword4" placeholder="Apellidos" value="<%= results[0].miembro_apellidos %>">
                  </div>

                 
                    <input type="hidden" class="form-control" id="inputPassword4" name="fecha_culto" placeholder="" value="<%= miculto[0].fecha_culto %>">
              

                  
                    <input type="hidden" readonly class="form-control" id="inputPassword4" name="miembro_id" placeholder="" value="<%= results[0].miembro_id %>">
                 

                

            <button type="submit" class="btn btn-primary">Registrar</button>
          </form>
          <% } %>


    </div>
  </div>

</main>

