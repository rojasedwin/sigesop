        <% if(!mostrarform) { %>    
        <form name="form_registroweb" id="form_registroweb" action="/searchcedula" method="post">
            <div class="card card-primary">
                <div class="card-header">
                  <h3 class="card-title">Buscar Cédula</h3>
                </div>
                <!-- /.card-header -->
                <!-- form start -->
                <form>
                  <div class="card-body">
                    <div class="form-group">
                      <label for="exampleInputEmail1">Cédula</label>
                      <input type="number" onkeypress="return event.charCode>=48 && event.charCode<=57" autofocus name="cedula" class="form-control" id="exampleInputEmail1" placeholder="Cédula">
                    </div>
                    
                    
                  </div>
                  <!-- /.card-body -->
  
                  <div class="card-footer">
                    <button type="submit" class="btn btn-primary">Buscar</button>
                  </div>
                </form>
              </div>
        </form> 

        <% } %>

          <% if(mostrarform) { %>
            <form name="form_registroweb" id="form_registroweb" action="/confirmarregistroweb" method="post">
                

            <div class="card card-primary">
                <div class="card-header">
                  <h3 class="card-title">Mis Datos</h3>
                </div>
 
                <form>
                  <div class="card-body">
                    <div class="form-group">
                      <label for="exampleInputEmail1">Cédula</label>
                      <input type="text" readonly class="form-control" id="exampleInputEmail1" placeholder="Cédula" value="<%= results[0].miembro_cedula %>">
                    </div>

                    <div class="form-group">
                        <label for="exampleInputEmail1">Nombre</label>
                        <input type="text" readonly class="form-control" id="exampleInputEmail1" placeholder="Nombre" value="<%= results[0].miembro_nombres %>">
                      </div>

                      <div class="form-group">
                        <label for="exampleInputEmail1">Apellido</label>
                        <input type="text" readonly class="form-control" id="exampleInputEmail1" placeholder="Apellido" value="<%= results[0].miembro_apellidos %>">
                      </div>

                      <input type="hidden" class="form-control" id="inputPassword4" name="fecha_culto" placeholder="" value="<%= miculto[0].fecha_culto %>">
              

                  
                    <input type="hidden" readonly class="form-control" id="inputPassword4" name="miembro_id" placeholder="" value="<%= results[0].miembro_id %>">
                    
                    
                  </div>
  
                  <div class="card-footer">
                    <button type="submit" class="btn btn-primary">Registrar</button>
                  </div>
                </form>
              </div>

          </form>
