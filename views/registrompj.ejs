  <div class="album py-1 bg-light">
    <div class="container">
       
       
        <form name="form_registrompj" id="form_registrompj" action="/saveeventompj" method="post">
            <div class="card card-primary">

                  <div class="card card-primary">
                    <div class="card-header">
                      <h3 class="card-title">Datos del Coordinador</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                      <form id="formregistro_mpj">
                        <div class="row">
                          <div class="col-sm-6">
                            <!-- text input -->
                            <div class="form-group">
                              <label>Nombre</label>
                              <input type="text" name="evento_nombre" class="form-control" placeholder="Ingrese Nombre" value="<%= mostrarDatosMpj==true ? nombre:'' %>">
                            </div>
                          </div>
                          <div class="col-sm-6">
                            <!-- text input -->
                            <div class="form-group">
                              <label>Apellido</label>
                              <input type="text" name="evento_apellido" class="form-control" placeholder="Ingrese Apellido" value="<%= mostrarDatosMpj==true ? apellido:'' %>">
                            </div>
                          </div>
                        </div>

                        <div class="row">
                          <div class="col-sm-6">
                            <!-- text input -->
                            <div class="form-group">
                              <label>Cédula</label>
                              <input type="number" onkeypress="return event.charCode>=48 && event.charCode<=57" ondrop="return false;" onpaste="return false;" autofocus  name="evento_cedula" class="form-control" placeholder="Ingrese Cédula" value="<%= mostrarDatosMpj==true ? cedula:'' %>">
                            </div>
                          </div>
                          <div class="col-sm-6">
                            <!-- text input -->
                            <div class="form-group">
                              <label>Teléfono</label>
                              <input type="text" data-inputmask='"mask": "(9999)999-99-99"' data-mask class="form-control" name="evento_telefono" id="evento_telefono" value="<%= mostrarDatosMpj==true ? telefono:'' %>">
                            </div>
                          </div>
                        </div>
                        

                        <div class="row">
                          <div class="col-sm-6">
                            <!-- text input -->
                            <div class="form-group">
                              <label>Email</label>
                              <input type="text" name="evento_email" class="form-control" placeholder="Ingrese Email" value="<%= mostrarDatosMpj==true ? email:'' %>">
                            </div>
                          </div>

                          <div class="col-sm-6">
                            <!-- select -->
                            <div class="form-group">
                              <label>Cargo</label>
                              <select name="evento_cargo" class="form-control">
                                <option value="">Seleccione</option>
                                <option value="Coordinador Regional">Coordinador Regional</option>
                                <option value="Coordinador Municipal">Coordinador Municipal</option>
                                <option value="Coordinador Parroquial">Coordinador Parroquial</option>
                               
                              </select>
                            </div>
                          </div>
                          
                          </div>

                          <div class="row">
                          
                            <div class="col-sm-6">
                              <div class="form-group">
                                <label>Estado</label>
                                <select name="evento_estado" class="form-control">
                                  <option value="">Seleccione</option>
                                  <option value="Amazonas">Amazonas</option>
                                  <option value="Anzoátegui">Anzoátegui</option>
                                  <option value="Apure">Apure</option>
                                  <option value="Aragua">Aragua</option>
                                  <option value="Barinas">Barinas</option>
                                  <option value="Bolívar">Bolívar</option>
                                  <option value="Delta Amacuro">Delta Amacuro</option>
                                  <option value="Distrito Capital">Distrito Capital</option>
                                  <option value="Falcón">Falcón</option>
                                  <option value="Guárico">Guárico</option>
                                  <option value="La Guaira">La Guaira</option>
                                  <option value="Lara">Lara</option>
                                  <option value="Mérida">Mérida</option>
                                  <option value="Miranda">Miranda</option>
                                  <option value="Monagas">Monagas</option>
                                  <option value="Nueva Esparta">Nueva Esparta</option>
                                  <option value="Portuguesa">Portuguesa</option>
                                  <option value="Sucre">Sucre</option>
                                  <option value="Táchira">Táchira</option>
                                  <option value="Trujillo">Trujillo</option>
                                 
                                  <option value="Yaracuy">Yaracuy</option>
                                  <option value="Zulia">Zulia</option>
                                  
                                 
                                </select>
                              </div>
                            </div>

                            <div class="col-sm-6">
                              <!-- text input -->
                              <div class="form-group">
                                <label>Si es Coordinador Municipal o Parroquial ¿Cuál Municipio o Parroquia?</label>
                                <input type="text" name="evento_coordinador_zona" class="form-control" placeholder="Ingrese Municipio o Parroquia" value="<%= mostrarDatosMpj==true ? zona:'' %>">
                              </div>
                            </div>

                            
  
                            
                          </div>

                                             

                        <div class="row">
                          
                          <div class="col-sm-6">
                            <!-- text input -->
                            <div class="form-group">
                              <label>Ministerio</label>
                              <input type="text" class="form-control" name="evento_ministerio_nombre" placeholder="Ingrese el nombre del Ministerio" id="evento_ministerio_nombre" value="<%= mostrarDatosMpj==true ? ministerio:'' %>">
                            </div>
                          </div>

                          <div class="col-sm-6">
                            <!-- text input -->
                            <div class="form-group">
                              <label>Cobertura o Paternidad Espiritual</label>
                              <input type="text" name="evento_cobertura" class="form-control" placeholder="Ingrese el nombre de su cobertura o Paternidad Espiritual" value="<%= mostrarDatosMpj==true ? cobertura:'' %>">
                            </div>
                          </div>



                        </div>

                       
                        
                      </form>
                    </div>
                    <!-- /.card-body -->
                  </div>
  
                  <div class="card-footer">
                    <button type="submit" class="btn btn-primary">Registrar</button>
                  </div>

              </div>
        </form> 


          


    </div>
  </div>

