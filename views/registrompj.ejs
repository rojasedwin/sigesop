<script>

$('[data-mask]').inputmask()

$.validator.setDefaults({
submitHandler: function () {
document.getElementById("form_registrompj").submit();
}
});

$('#form_registrompj').validate({
    rules: {
      evento_nombre: {
        required: true
      },
      evento_apellido: {
        required: true
      },
      evento_email: {
        required: true
      },
      evento_telefono: {
        required: true
      },

      evento_cedula: {
        required: true
      },

      evento_cargo: {
        required: true
      },
      evento_estado: {
        required: true
      },
      evento_ministerio_nombre: {
        required: true
      },
      evento_cobertura: {
        required: true
      },
     
    },
    messages: {
      evento_nombre: {
        required: "Ingrese un Nombre"
 
      },
      evento_apellido: {
        required: "Ingrese un Apellido"
 
      },
      evento_cedula: {
        required: "Ingrese una Cédula"
 
      },
      evento_email: {
        required: "Ingrese un Email"
 
      },
      evento_telefono: {
        required: "Ingrese un Teléfono"
 
      },
      evento_cargo: {
        required: "Seleccione un Cargo"
 
      },
      evento_estado: {
        required: "Seleccione un Estado"
 
      },
      evento_ministerio_nombre: {
        required: "Ingrese un nombre del Ministerio"
 
      },
      evento_cobertura: {
        required: "Ingrse un nombre de la cobertura o Paternidad Espiritual"
 
      }
     
     
    },
    errorElement: 'span',
    errorPlacement: function (error, element) {
      error.addClass('invalid-feedback');
      element.closest('.form-group').append(error);
    },
    highlight: function (element, errorClass, validClass) {
      $(element).addClass('is-invalid');
    },
    unhighlight: function (element, errorClass, validClass) {
      $(element).removeClass('is-invalid');
    }
  });


  $('body').on('keyup','.is_number',function(){
 				if($(this).val().indexOf('.')!=-1)
				{
						$(this).val($(this).val().replace(".",""));
				}

        if($(this).val().indexOf('-')!=-1)
				{
						$(this).val($(this).val().replace("-",""));
				}
		});

</script>
      
